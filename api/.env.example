# --- Embedding Provider Settings ---
# Defines which embedding provider to use ("openai", "gemini", etc.)
EMBEDDING_PROVIDER="openai"

# API key for the selected provider
EMBEDDING_PROVIDER_API_KEY="your_api_key_here"

# Model for the selected provider
EMBEDDING_PROVIDER_MODEL="text-embedding-3-small"

# (Optional) Embedding dimensions, if supported by the model
EMBEDDING_PROVIDER_DIMENSIONS=1536

# --- Database Settings ---
DATABASE_URL="postgresql://bubblebot:bubblebot_dev@localhost:5432/bubblebot_db"

# --- Development settings ---
DEBUG=True

LOG_LEVEL=INFO
